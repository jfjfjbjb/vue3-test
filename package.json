    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.1",
    "less": "^3.7.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.0",
